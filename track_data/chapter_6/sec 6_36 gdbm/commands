./configure --prefix=/usr --enable-libgdbm-compat

time make -j4 >make.log 2>make_err.log
{
real	0m4.682s
user	0m12.860s
sys	0m0.964s
}

time make check >make_check.log 2>make_check_err.log
{
real	0m7.786s
user	0m2.956s
sys	0m0.564s
}

time make install >make_install.log 2>make_install_err.log
{
real	0m0.753s
user	0m0.420s
sys	0m0.036s
}
